{
  "void": {
    "prefix": ["void()"],
    "body": [
      "void ${1}(${2})",
      "{",
      "  ${0}",
      "}"
    ]
  },
  "Include": {
    "prefix": ["#inc<"],
    "body": [
      "#include <${1:stdio}.h>",
      "${0}"
    ]
  },
  "Struct": {
    "prefix": ["tstruct"],
    "body": [
      "typedef struct ${1}{",
      "  ${0}",
      "} ${2:Alias};"
    ]
  }
}
