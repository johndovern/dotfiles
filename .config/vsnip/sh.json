{
  "dev/null": {
    "prefix": [">/dev/n", ">void"],
    "body": [
      ">/dev/null 2>&1"
    ]
  },
  "send_prog": {
    "prefix": ["send()"],
    "body": [
      "send_prog() {",
      "  case \"\\$1\" in",
      "    -e) B=\"\\${R}\" ; shift ;;",
      "    -p) B=\"\\${G}\" ; shift ;;",
      "    *) echo \"NO OPTION\" ; return 1 ;;",
      "  esac",
      "",
      "  echo -e \"\\${B}\\${*}\\${E}\"",
      "}"
    ]
  },
  "Bash Colors": {
    "prefix": ["excolors", "bcolor"],
    "body": [
      "export R=\"\\e[0;31m\"",
      "export G=\"\\e[0;32m\"",
      "export E=\"\\e[0m\"",
      "${0}"
    ]
  },
  "get_opts": {
    "prefix": ["get_opts()"],
    "body": [
      "get_opts() {",
      "  while :; do",
      "    if [[ \\$# -gt 0 ]]; then",
      "      case \"\\$1\" in",
      "        ${1}) ",
      "          ${2}",
      "          ;;",
      "        *)",
      "          printf 'Unknown option passed: %s\\n' \"\\$1\" >&2",
      "          return 1",
      "          ;;",
      "      esac",
      "    else",
      "      break",
      "    fi",
      "  done",
      "}"
    ]
  },
  "If-then": {
    "prefix": ["ifthen"],
    "body": [
      "if [[ ${1} ]]; then",
      "${0}",
      "fi"
    ]
  },
  "Main": {
    "prefix": ["main()"],
    "body": [
      "main() {",
      "  ${0}",
      "}",
      "",
      "main \"\\${@}\" && exit 0 || exit 1"
    ]
  }
}
