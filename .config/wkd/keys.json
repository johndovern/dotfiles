    "s -> System": {
        "D -> Dangerous": {
            "d -> Display off": "sysact Display-off",
            "L -> Leave dwm": "sysact Leave",
            "R -> Reboot": "sysact Reboot",
            "s -> Sleep": "sysact Sleep",
            "S -> shutdown": "sysact Shutdown"
        },
        "h -> Hide": "dwmc focusnmon 1 ; dwmc viewex 0 ; dwmc focusnmon 0",
        "l -> Lock": "sleep 0.1 ; sysact Lock",
        "r -> Renew dwm": "sysact Renew-dwm",
        "u -> Unhide": "dwmc focusnmon 1 ; dwmc viewex 8 ; dwmc focusnmon 0"
    },
    "t -> Toggle": {
