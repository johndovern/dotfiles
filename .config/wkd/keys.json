                "m -> dmenu": "emacs \"~/.local/src/dmenu\""
            }
        }
    },
    "m -> mpv": {
        "d -> Float": "mpv-opts --float",
        "f -> Focus": "mpv-opts --focus",
        "l -> Playlists": {
            "g -> ygyl": "mpv-playlist --shuffle \"$HOME/.config/mpv/playlists/groove.m3u\"",
            "o -> anime groove": "mpv-playlist --shuffle \"$HOME/.config/mpv/playlists/groovy.m3u\""
        },
        "m -> Move right": "mpv-opts --move",
        "M -> Move left": "mpv-opts --move 0",
        "o -> mpv opts": {
            "f -> Fullscreen": "mpv-opts --full-screen",
            "s -> Sticky": "mpv-opts --sticky",
            "S -> Shuffle": "mpv-opts --shuffle"
        },
        "p -> Pick mpv": {
            "s -> Seek": {
                "l -> Seek +10s": "mpv-seek 10 ; wkd -p ms",
                "L -> Seek +60s": "mpv-seek 60 ; wkd -p ms",
                "h -> Seek -10s": "mpv-seek -10 ; wkd -p ms",
                "H -> Seek -60s": "mpv-seek -60 ; wkd -p ms",
                "n -> Next": "mpv-next --pick",
                "p -> Prev": "mpv-prev --pick"
            },
            "t -> Toggle": {
                "m -> Toggle music": "mpv-toggle --music",
                "t -> Toggle pick": "mpv-toggle --pick",
                "u -> Toggle umpv": "mpv-toggle --umpv"
            }
        },
        "q -> quit pick": "mpv-quit",
        "s -> Seek": {
            "l -> Seek +10s": "mpv-seek -c 10 ; wkd -p ms",
            "L -> Seek +60s": "mpv-seek -c 60 ; wkd -p ms",
            "h -> Seek -10s": "mpv-seek -c -10 ; wkd -p ms",
            "H -> Seek -60s": "mpv-seek -c -60 ; wkd -p ms",
            "n -> Next": "mpv-next --pick ; wkd -p ms",
            "p -> Prev": "mpv-prev --pick ; wkd -p ms"
        },
        "t -> Smart toggle": "mpv-toggle"
    },
    "n -> nvim": {
        "n -> Empty nvim": "st -e nvim",
        "t -> Temp notes": "st -e nvim -c VimwikiIndex -c \"VimwikiGoto temp-notes\"",
        "w -> VimwikiIndex": "st -e nvim -c VimwikiIndex"
    },
    "o -> Open": {
        "h -> htop": "st -e htop",
        "l -> lf": "st -n lf_run -e lfrun",
        "m -> music": {
            "n -> ncmpcpp": "st -e ncmpcpp"
        },
        "p -> Pulsemixer": "st -e pulsemixer",
        "s -> Screen shot": {
            "c -> Copy Shot": {
                "c -> Current window": "maim -i \"$(xdotool getactivewindow)\" | xclip -selection clipboard -t image/png",
                "f -> Full screen": "maim | xclip -selection clipboard -t image/png",
                "s -> Selected area": "maim -s | xclip -selection clipboard -t image/png"
            },
            "f -> Full screen": "maim pic-full-$(date '+%y%m%d-%H%M-%S').png && notify-send \"Screenshot of desktop taken\"",
            "s -> Selected area": "maim -s pic-selected-\"$(date '+%y%m%d-%H%M-%S').png\"",
            "w -> Current window": "maim -i \"$(xdotool getactivewindow)\" pic-window-\"$(date '+%y%m%d-%H%M-%S').png\""
        }
    },
    "p -> mpc": {
